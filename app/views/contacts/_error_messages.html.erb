<% if @contact.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      Formularz zawiera <%= I18n.t :error, count: @contact.errors.count %>.
    </div>
    <ul>
    <% @contact.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>